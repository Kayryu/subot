{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": ["<rootDir>/node_modules/(?!(lodash-es|other-es-lib))"]
}
